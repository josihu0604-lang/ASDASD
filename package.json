{
  "name": "webapp",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:monitor": "./scripts/dev-server-monitor.sh",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "health": "./scripts/health-check.sh",
    "errors": "./scripts/error-analyzer.sh",
    "fix:all": "./scripts/auto-fix.sh all",
    "fix:deps": "./scripts/auto-fix.sh deps",
    "fix:cache": "./scripts/auto-fix.sh cache",
    "fix:port": "./scripts/auto-fix.sh port",
    "fix:types": "./scripts/auto-fix.sh types",
    "logs:clean": "./scripts/auto-fix.sh logs",
    "test": "vitest",
    "test:unit": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:ui": "playwright test --ui",
    "test:smoke": "./scripts/test-api-routes.sh",
    "test:all": "npm run test:unit && npm run test:e2e",
    "deploy": "vercel",
    "deploy:prod": "vercel --prod",
    "vercel:env": "vercel env pull .env.local",
    "vercel:inspect": "vercel inspect",
    "vercel:logs": "vercel logs"
  },
  "dependencies": {
    "@prisma/client": "^6.19.0",
    "lucide-react": "^0.553.0",
    "mapbox-gl": "^3.16.0",
    "next": "16.0.2",
    "ngeohash": "^0.6.3",
    "prisma": "^6.19.0",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-map-gl": "^7.1.7",
    "supercluster": "^8.0.1",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@playwright/test": "^1.56.1",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20.19.25",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitest/coverage-v8": "^4.0.8",
    "@vitest/ui": "^4.0.8",
    "eslint": "^9",
    "eslint-config-next": "16.0.2",
    "tailwindcss": "^4",
    "typescript": "^5",
    "vercel": "^48.9.1",
    "vitest": "^4.0.8"
  }
}
